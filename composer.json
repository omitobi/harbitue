{
    "name": "omitobisam/laravel-harbitue",
    "description": "An Http client with the power of collections for your Jsonable requests",
    "license": "MIT",
    "keywords": [
        "harbitue",
        "Http",
        "Guzzle",
        "Collection",
        "json"
    ],
    "homepage": "https://omitobi.github.io/laravel-harbitue/",
    "support": {
        "issues": "https://github.com/omitobi/laravel-harbitue/issues",
        "source": "https://github.com/omitobi/laravel-harbitue/issues"
    },
    "authors": [
        {
            "name": "Oluwatobi Samuel Omisakin",
            "email": "omisakin.oluwatobi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^6.3",
        "tightenco/collect": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "Harbitue\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
